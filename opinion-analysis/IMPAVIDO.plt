        <accion descripcion=" Pasa Tena, toma asiento. Tena ve que la libreta sigue en el sitio donde la dejó. Va a sentarse en el mismo asiento que antes, pero Dimas le ofrece con la mano otro más cercano a él, Tena obedece intentando no demostrar su ansiedad." duracion="14" parte="1" tiempoDescrip="12" infoExtraAccion=" ">
          <charDialog char="DIMAS" tdialog="2" texto="Tena, ¿tú crees en el destino? " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Tena, ¿tú crees en el destino? conviertes en Schopenhauer?" duracion="14" parte="2" tiempoDescrip="10" infoExtraAccion=" ">
          <charDialog char="TENA" tdialog="2" texto="¿Te dejo solo un par de horas y te " vozPersonaje=""/>
          <charDialog char="DIMAS" tdialog="2" texto="Siempre he creído que es un invento de " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Siempre he creído que es un invento de los pobres para no sentirse responsables de su mala suerte." duracion="10" parte="3" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="TENA" tdialog="4" texto="Me encantaría quedarme a charlar, pero " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Me encantaría quedarme a charlar, pero sólo me he pasado para ver si ya sabías algo sobre el atraco. Dimas se pone en el camino de Tena hacia la libreta, y la coge de la mesa. Tena palidece." duracion="14" parte="4" tiempoDescrip="12" infoExtraAccion=" ">
          <charDialog char="DIMAS" tdialog="2" texto="Ahora soy dueño de mi destino. Elijo " vozPersonaje=""/>
