        <charPoint char="DANI" label="0" tpar="100"/>
        <accion descripcion="  El coche llega a la calle entre gritos de alegría." duracion="8" parte="0" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="DANI" tdialog="4" texto="¡Eres un genio! " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Se escucha un berrido." duracion="6" parte="1" tiempoDescrip="2" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="4" texto="Lo sé, pero no te pongas histérica. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Lo sé, pero no te pongas histérica. Los quejidos que molestan a Rai son en realidad el preludio del llanto de un bebé, que ocupa su sillita en la parte trasera. El coche frena en seco." duracion="12" parte="2" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="4" texto="Llévate el coche. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Llévate el coche." duracion="18" parte="3" tiempoDescrip="12" infoExtraAccion=" ">
          <charDialog char="DANI" tdialog="2" texto="¿Qué vas a hacer? " vozPersonaje=""/>
          <charDialog char="RAI" tdialog="2" texto="¿Prefieres adoptar? " vozPersonaje=""/>
          <charDialog char="DANI" tdialog="2" texto="Te van a coger. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Te van a coger." duracion="6" parte="4" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Vete ya o nos pillarán a los dos. Nos " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Vete ya o nos pillarán a los dos. Nos vemos luego. Dani aprovecha que las cabezas están juntas para pasar el colgante de su cuello al de Rai." duracion="14" parte="5" tiempoDescrip="12" infoExtraAccion=" ">
          <charDialog char="DANI" tdialog="2" texto="Te dará suerte. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Te dará suerte. Rai sonríe enternecido y sale del coche de un salto, lo rodea y saca al bebé. Dani ocupa el asiento del piloto y sale disparada en cuanto Rai cierra la puerta. Rai baja la rampa sujetando al bebé como si fuera radioactivo." duracion="10" parte="6" tiempoDescrip="10" infoExtraAccion=" "/>
      </pointGroup>
    </timeSlice>
    <timeSlice step="3" stepReal="4" time="4" duracion="92" letra="." numeroCabecera="3" cabecera="..">
      <pointGroup y="819.2" dummy="1" pin="0" ambiente="-1" principal="1" tipo="-1" tipoEscena="NORMAL" urlFoto="" indiceAsignado="4">
