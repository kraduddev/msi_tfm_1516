        </accion>
        <accion descripcion=" Debe estar dentro poniéndose las rodilleras. Anda, dame la pasta." duracion="8" parte="7" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Tena, no te estoy entendiendo una puta " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Tena, no te estoy entendiendo una puta mierda. ¿Qué pasta?" duracion="8" parte="8" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="TENA" tdialog="2" texto="Diez mil por cabeza. Te lo acabo de " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Diez mil por cabeza. Te lo acabo de decir." duracion="8" parte="9" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Entonces, si gano ¿me llevo?... " vozPersonaje=""/>
