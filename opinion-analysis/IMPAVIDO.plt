        <charPoint char="RAI" label="0" tpar="100"/>
        <charPoint char="DANI" label="0" tpar="100"/>
        <charPoint char="TENA" label="0" tpar="100"/>
        <accion descripcion="  El niño no para de llorar y Rai intenta calmarlo mientras corre." duracion="8" parte="0" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Venga, si ni siquiera tienes carné. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Venga, si ni siquiera tienes carné. Al final de la rampa se encuentra a la madre con la zapatilla de Rai en la mano. El niño llora cada vez más fuerte." duracion="10" parte="1" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Señora, le va a quedar grande. Se la " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Señora, le va a quedar grande. Se la cambio por Pavarotti... Antes de que termine de hablar el cañón de una pistola le presiona la sien. Un orondo GUARDA DE SEGURIDAD (55), visiblemente nervioso, le está apuntando." duracion="20" parte="2" tiempoDescrip="16" infoExtraAccion=" ">
          <charDialog char="GUARDA DE SEGURIDAD" tdialog="2" texto="¡Suéltalo! " vozPersonaje=""/>
          <charDialog char="RAI" tdialog="2" texto="Vale. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Vale." duracion="12" parte="3" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="GUARDA DE SEGURIDAD" tdialog="2" texto="¡Que lo sueltes, joder! " vozPersonaje=""/>
          <charDialog char="RAI" tdialog="2" texto="Que sí, que lo suelto pero... " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Que sí, que lo suelto pero... te voy a reventar la cabeza! señora a por él o lo coges tú al segundo bote." duracion="24" parte="4" tiempoDescrip="14" infoExtraAccion=" ">
          <charDialog char="GUARDA DE SEGURIDAD" tdialog="4" texto="¡Me cago en la puta! ¡Como no lo sueltes " vozPersonaje=""/>
          <charDialog char="RAI" tdialog="2" texto="¡Que lo suelto, coño! Pero o viene la " vozPersonaje=""/>
          <charDialog char="GUARDA DE SEGURIDAD" tdialog="4" texto="Señora… " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Señora… La mujer suelta la zapatilla y recoge al bebé entre lágrimas." duracion="10" parte="5" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="¿Me puedo poner la zapatilla? Por favor. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" El guarda de seguridad le golpea con la culata de la pistola, y la imagen se congela sobre el rostro desfigurado de Rai." duracion="8" parte="6" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="TENA" tdialog="2" texto="Aparte del robo, a Rai le acusaron de " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Aparte del robo, a Rai le acusaron de intento de secuestro, y como no delató a Daniela, le agravaron la pena. Aún así, se habría librado de la cárcel si no fuera porque la amable señora a la que le mangaron el coche resultó ser la concejala de Consumo, Comercio e Industria. Salió en la tele." duracion="16" parte="7" tiempoDescrip="16" infoExtraAccion=" "/>
      </pointGroup>
    </timeSlice>
    <timeSlice step="4" stepReal="5" time="5" duracion="266" letra="." numeroCabecera="4" cabecera="..">
      <pointGroup y="204.8" dummy="1" pin="0" ambiente="-1" principal="1" tipo="-1" tipoEscena="NORMAL" urlFoto="" indiceAsignado="1">
        <charPoint char="RAI" label="0" tpar="100"/>
        <charPoint char="SÁNCHEZ" label="0" tpar="100"/>
        <charPoint char="MANRIQUE" label="0" tpar="100"/>
        <charPoint char="TENA" label="0" tpar="100"/>
        <accion descripcion="  Rai está en el patio de la cárcel leyendo una carta. Una pelota de baloncesto rebota muy cerca de él. Un par de presos se acercan, el más alto, SÁNCHEZ (35), se dirige a Rai." duracion="12" parte="0" tiempoDescrip="10" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="2" texto="¡Eh, pelota! " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Rai la recoge, la lanza y vuelve a su carta. Sánchez coge la pelota al vuelo y se acerca a él." duracion="12" parte="1" tiempoDescrip="10" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="2" texto="¿Cuánto mides? Nos falta un alero. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Soy Sánchez." duracion="6" parte="2" tiempoDescrip="2" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="4" texto="Rai. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Rai." duracion="6" parte="3" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="2" texto="Tú eres el que apadrinó al hijo de la " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Tú eres el que apadrinó al hijo de la concejala, ¿no? Rai asiente con la cabeza." duracion="12" parte="4" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="4" texto="¿Te han hecho la visita guiada? " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Rai no dice nada, todavía no sabe si Sánchez es de fiar." duracion="6" parte="5" tiempoDescrip="2" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="4" texto="Para empezar, aquí no nos van los " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Para empezar, aquí no nos van los deportes individuales, tienes que fichar por algún equipo o alguien jugará a las damas contigo. ¿Me sigues? En ese momento se acerca MANRIQUE (60), un preso de mirada astuta que le quita la pelota a Sánchez." duracion="16" parte="6" tiempoDescrip="14" infoExtraAccion=" ">
          <charDialog char="MANRIQUE" tdialog="2" texto="Tus amigas te esperan, Sánchez. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Tus amigas te esperan, Sánchez." duracion="6" parte="7" tiempoDescrip="2" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="4" texto="Vale, Manrique, tranquilo. Me pareció " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Vale, Manrique, tranquilo. Me pareció que querría jugar en la NBA." duracion="8" parte="8" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="MANRIQUE" tdialog="4" texto="Es campeón de curling. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Es campeón de curling." duracion="12" parte="9" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="SÁNCHEZ" tdialog="2" texto="¿De qué? " vozPersonaje=""/>
          <charDialog char="MANRIQUE" tdialog="2" texto="Esos que le van fregando el suelo a la " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Esos que le van fregando el suelo a la piedra esa con asa… que te pires. Manrique observa a Sánchez y cuando está lo suficientemente lejos se dirige a Rai. jubilación en el módulo D?" duracion="18" parte="10" tiempoDescrip="16" infoExtraAccion=" ">
          <charDialog char="MANRIQUE" tdialog="2" texto="¿Y a ti qué te pasa? ¿Quieres pasar la " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Y guarda esa carta: es como llevar un cartel de &quot;culito fresco&quot; en la frente." duracion="8" parte="11" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="4" texto="Mi madre me enseñó a no aceptar regalos " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Mi madre me enseñó a no aceptar regalos de desconocidos." duracion="8" parte="12" tiempoDescrip="6" infoExtraAccion=" ">
          <charDialog char="MANRIQUE" tdialog="2" texto="Ya. Y lo de ir robando coches por ahí " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Ya. Y lo de ir robando coches por ahí pitillo, aún no he encontrado la máquina de tabaco." duracion="20" parte="13" tiempoDescrip="16" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="¡Oye, Obi Wan! Te voy a aceptar ese " vozPersonaje=""/>
          <charDialog char="MANRIQUE" tdialog="2" texto="Entre el jacuzzi y el mini golf. " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Entre el jacuzzi y el mini golf." duracion="6" parte="14" tiempoDescrip="4" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="Si me das fuego te dejo que me cuentes " vozPersonaje=""/>
        </accion>
        <accion descripcion=" Si me das fuego te dejo que me cuentes tu historia. Lo de siempre ¿no?, error judicial y mala suerte." duracion="10" parte="15" tiempoDescrip="8" infoExtraAccion=" ">
          <charDialog char="MANRIQUE" tdialog="2" texto="La suerte no existe. Un ejemplo, si das " vozPersonaje=""/>
        </accion>
        <accion descripcion=" La suerte no existe. Un ejemplo, si das el nombre de tu cómplice y devuelves el coche, no tienes la &quot;mala suerte&quot; de cumplir la condena íntegra." duracion="18" parte="16" tiempoDescrip="14" infoExtraAccion=" ">
          <charDialog char="RAI" tdialog="2" texto="¿Cómo sabes tú eso? " vozPersonaje=""/>
