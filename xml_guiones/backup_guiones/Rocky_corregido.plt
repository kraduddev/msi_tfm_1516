        <timeSlice cabecera=" GOLDMILL'S GYM .INT." duracion="52" letra="." numeroCabecera="77" step="77" stepReal="77" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" GOLDMILL'S GYM " pin="0" principal="1" tipo="0" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="MICKEY" label="1" tpar="100"/>
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <charPoint char="BENNY" label="1" tpar="100"/>
                <charPoint char="PAULIE" label="1" tpar="100"/>
                <accion descripcion=" Rocky has just completed exercising with the medicine ball... Dino hands him a towel... Paulie crosses the gym as Rocky heads to the steam room... A thin man crosses the gym and Mickey nods." duracion="12" infoExtraAccion="" parte="0" tiempoDescrip="10">
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="What cha say, Benny -- Meet " vozPersonaje=""/>
                </accion>
                <accion descripcion=" cutman, Benny Stein." duracion="12" infoExtraAccion="" parte="1" tiempoDescrip="8">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Yo, Benny. " vozPersonaje=""/>
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="Check the eyes, Ben. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Benny checks the skin around Rocky's eyes." duracion="14" infoExtraAccion="" parte="2" tiempoDescrip="6">
                    <charDialog acotacion="" char="BENNY" tdialog="6" texto="Ain't bad.  Seen worse -- Cover up an' things should be okay. " vozPersonaje=""/>
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="Take a shower. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Heading towards the shower, Rocky is intercepted by Paulie who traverses the gym.  He stops Rocky at the shower room entrance." duracion="14" infoExtraAccion="" parte="3" tiempoDescrip="8">
                    <charDialog acotacion="" char="PAULIE" tdialog="6" texto="Yo, Rock, I think I found an angle to make some bread usin' ya name, ya mind? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Exhausted, Rocky studies Paulie's face a moment and nods strides off." duracion="6" parte="4" tiempoDescrip="6"/>
            </pointGroup>
        </timeSlice>
