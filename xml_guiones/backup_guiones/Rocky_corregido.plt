                </accion>
                <accion descripcion=" Rocky walks over to Adrian's bedroom door and begins speaking to the enclosed girl." duracion="20" infoExtraAccion="" parte="6" tiempoDescrip="6">
                    <charDialog acotacion="" char="ROCKY" tdialog="14" texto="... Yo, Adrian, it's me, Rocky... Ah, ah -- Ah, it's kinda hard for me to think of somethin' to say, y'know -- 'Cause I never talked to a door before, I mean whatta ya say to a door. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky turns away and begins to walk off." duracion="10" infoExtraAccion="" parte="7" tiempoDescrip="4">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="... Maybe I better forget it. " vozPersonaje=""/>
                    <charDialog acotacion="" char="PAULIE" tdialog="2" texto="Try again, c'mon, try again. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky goes over to the door and begins speaking again." duracion="24" infoExtraAccion="" parte="8" tiempoDescrip="4">
                    <charDialog acotacion="" char="ROCKY" tdialog="20" texto="Ah, Adrian, I know ya ain't too happy at this moment, but would ya do me a favor -- I ain't got nobody to spend Thanksgivin' with -- How 'bout you an' me goin' out -- Get somethin' to eat, maybe laugh a little, who knows... Would ya like, I dunno, go out together? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian opens the door... She already has her winter coat on." duracion="6" infoExtraAccion="" parte="9" tiempoDescrip="2">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="We'll have a good time. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Paulie opens the front door and gives Rocky an encouraging smile..." duracion="10" infoExtraAccion="" parte="10" tiempoDescrip="6">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="What's ya sister like to do? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Ice skate." duracion="14" infoExtraAccion="" parte="11" tiempoDescrip="8">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="I didn't want no turkey anyway. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="It's Thanksgiving. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="To you, to me it's Thursday. " vozPersonaje=""/>
                </accion>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" ICE SKATING RINK .INT." duracion="104" letra="." numeroCabecera="36" step="36" stepReal="36" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" ICE SKATING RINK " pin="0" principal="1" tipo="0" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <charPoint char="ADRIAN" label="1" tpar="100"/>
                <charPoint char="CLEANER" label="1" tpar="100"/>
                <accion descripcion=" Rocky and Adrian enter a deserted skating rink." duracion="16" infoExtraAccion="" parte="0" tiempoDescrip="8">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Looks quiet, y'know. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="I think it's closed. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="I think mebbe we're early or somethin' -- " vozPersonaje=""/>
                </accion>
                <accion descripcion=" From across the rink a CLEANING MAN yells at them." duracion="38" infoExtraAccion="" parte="1" tiempoDescrip="6">
                    <charDialog acotacion="" char="CLEANER" tdialog="6" texto="Hey, whatta ya doin' here -- we're closed... Yo, we're closed! " vozPersonaje=""/>
                    <charDialog acotacion="(yells back)" char="ROCKY" tdialog="6" texto="Are ya closed to the General Public or to just everybody. " vozPersonaje=""/>
                    <charDialog acotacion="(starts to walk over)" char="CLEANER" tdialog="10" texto="Hey, the rink is empty 'cause we're closed -- ya ain't allowed in here so do me a favor an' not stay here. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="Wait here, gotta smooth this guy out. " vozPersonaje=""/>
                    <charDialog acotacion="(softly)" char="ADRIAN" tdialog="6" texto="We could go somewhere else an' -- " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky approaches the Cleaner." duracion="50" infoExtraAccion="" parte="2" tiempoDescrip="18">
                    <charDialog acotacion="" char="CLEANER" tdialog="4" texto="Yo, pal, what's with you -- The place ain't operatin'. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="12" texto="Listen, I gotta problem.  This girl ain't feelin' well, y'know -- The doctor says she should exercise, y'know once in awhile an' ice skatin' is the best thing -- " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="This a con? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="Look at her, ya can see she ain't feelin' good -- needs a few minutes exercise -- " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Few minutes? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="... Ten minutes. " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Ten minutes for ten dollars. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="... Yeah, give 'er the Blades. " vozPersonaje=""/>
                </accion>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" ICE SKATING RINK .INT." duracion="226" letra="." numeroCabecera="37" step="37" stepReal="37" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" ICE SKATING RINK " pin="0" principal="1" tipo="0" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="ADRIAN" label="1" tpar="100"/>
                <charPoint char="CLEANER" label="1" tpar="100"/>
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <accion descripcion=" Adrian has on skates.  Rocky follows her onto the ice.  He wears street shoes." duracion="10" infoExtraAccion="" parte="0" tiempoDescrip="8">
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="... Aren't you skating? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Ain't skated since I was fifteen -- That's when I started fightin' -- gotta watch the ankles.  Yeah, fightin' use to be tops with me, but no more.  All I wanted to prove was I weren't no bum -- That I had the stuff to make a good pro." duracion="22" infoExtraAccion="" parte="1" tiempoDescrip="20">
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="And you never got the chance? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" The Cleaner yells from the sideline." duracion="42" infoExtraAccion="" parte="2" tiempoDescrip="12">
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Nine minutes! " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="20" texto="Hey, I ain't cryin'... I still fight.  Kinda do it like a hobby.  See I'm a natural southpaw an' most pugs won't fight a southpaw 'cause we mess up their timin' an' look awkward -- Southpaw means lefthanded... But I guess in the long run things probably worked out for the best, right? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="4" texto="But you never had a chance to prove yourself. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Absolutely. " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Eight minutes! " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian slips and Rocky breaks her fall." duracion="54" infoExtraAccion="" parte="3" tiempoDescrip="18">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="I just dislocated my finger. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Ohh! " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Seven minutes! " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="20" texto="It ain't your fault -- I originally done it in the Baby Crenshaw fight. (opens his wallet) That's me fightin' Big Baby Crenshaw -- Big Baby was the size of an airplane an' I broke my hands on his head -- I lost, but it's a nice picture, don't ya think? " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Six! " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="How 'bout some Cokes? " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Cost ya a buck. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="This guy is beautiful -- get the Cokes. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian does a slight turn and Rocky has to speed up and nearly falls." duracion="18" infoExtraAccion="" parte="4" tiempoDescrip="4">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="14" texto="See, I ain't graceful, y'know -- I don't move good -- Stink as a dancer too -- But I can really swat, I hit hard,            real hard, but I'm a southpaw an' nobody wants to fight a southpaw! -- Havin' a good time? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian nods and is very nervous... The Cleaner moves carefully across the ice." duracion="28" infoExtraAccion="" parte="5" tiempoDescrip="10">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="6" texto="Y'know how I got started in the fight racket? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="By accident? " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Here -- Three minutes. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="8" texto="Yeah -- My ol' man who was never the sharpest told me -- I weren't born with much brain so I better use my body. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" For the first time, Adrian laughs." duracion="22" infoExtraAccion="" parte="6" tiempoDescrip="6">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="What's funny? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="10" texto="My mother told me just the opposite.  She said, 'You weren't born with much of a body so you'd better develop your brain.' " vozPersonaje=""/>
                    <charDialog acotacion="" char="CLEANER" tdialog="2" texto="Time! " vozPersonaje=""/>
                </accion>
                <accion descripcion=" The Cleaner walks over as Rocky and Adrian get off the ice." duracion="12" infoExtraAccion="" parte="7" tiempoDescrip="4">
                    <charDialog acotacion="(continuing)" char="CLEANER" tdialog="4" texto="That's ten bucks. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="I must be goin' deaf 'cause I thought ya said, 'ten cents.' " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky hands him a dime." duracion="10" infoExtraAccion="" parte="8" tiempoDescrip="4">
                    <charDialog acotacion="(weakly)" char="CLEANER" tdialog="4" texto="... How 'bout for the Cokes? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Charge it. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky and Adrian start to leave.  Rocky pauses at the door and fishes into his pocket and hands over eleven dollars." duracion="8" infoExtraAccion="" parte="9" tiempoDescrip="4">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="Had ya goin', didn't I, huh? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" He grins and leaves." duracion="2" parte="10" tiempoDescrip="2"/>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" STREET .EXT." duracion="122" letra="." numeroCabecera="38" step="38" stepReal="38" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" STREET " pin="0" principal="1" tipo="1" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <charPoint char="ADRIAN" label="1" tpar="100"/>
                <accion descripcion="" duracion="62" infoExtraAccion="" parte="0" tiempoDescrip="18">
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="Some people are very shy by nature. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="... I suppose. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="I would say you're very shy bu nature. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="... I suppose. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="Some people think bein' shy is a disease, but it don't bother me. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="It doesn't bother me either. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="12" texto="Then why did I bother bringin' it up?  'Cause I'm dumb, that's why... Y'know, I think we make a real sharp coupla coconuts -- I'm dumb an' you're shy. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="6" texto="... It is just hard for me to understand why anybody wants to be a fighter. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="Ya gotta be a little soft to wanna be a pug... It's a racket where ya' almost " vozPersonaje=""/>
                </accion>
                <accion descripcion=" guaranteed to end up a bum." duracion="60" infoExtraAccion="" parte="1" tiempoDescrip="14">
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="I don't think you're a bum. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="8" texto="... I'm at least half a bum. Yeah, fightin' is a crazy racket.  The roughest part is the mornin' after. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Morning after? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="24" texto="After a rough fight, ya' nothin' but a large wound. Sometimes I feel like callin' a taxi to drive me from my bed to the bathroom... Ya' eyes hurt, ya' ears hurt, ya' hair even hurts... But the thing I'm proud of is I been in over sixty fights an' never had a busted nose -- Bent an' twisted an' bitten but never broke... That's rare. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="4" texto="Why do you do it if it hurts so bad? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="... Guess. " vozPersonaje=""/>
                    <charDialog acotacion="(pause)" char="ADRIAN" tdialog="4" texto="'Cause you can't sing or dance? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky smiles." duracion="2" parte="2" tiempoDescrip="2"/>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" ROCKY'S APARTMENT .INT." duracion="246" letra="." numeroCabecera="39" step="39" stepReal="39" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" ROCKY'S APARTMENT " pin="0" principal="1" tipo="0" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <charPoint char="ADRIAN" label="1" tpar="100"/>
                <charPoint char="PAULIE" label="1" tpar="100"/>
                <accion descripcion=" Rocky and Adrian enter his one-room apartment... She is nervous and taken aback by the bleakness of the room... Rocky goes to the icebox." duracion="14" infoExtraAccion="" parte="0" tiempoDescrip="10">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Would ya like a glass of water? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="... No thanks. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian looks at the mirror above Rocky's dresser.  She sees a high school photo of Rocky.  He once was handsome and smooth-faced... Rocky steps up behind her and his face is reflected in the mirror. He turns on his cheap RECORD PLAYER... He reaches into the turtle bowl." duracion="86" infoExtraAccion="" parte="1" tiempoDescrip="36">
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="Here's the guys I was tellin' ya about -- This is Cuff an' Link. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="I sold them to you. " vozPersonaje=""/>
                    <charDialog acotacion="(very embarrassed)" char="ROCKY" tdialog="14" texto="... Oh, yeah, I bought the whole kit -- Yeah, ya sold me the turtles, the bowl, an' the mountain -- I had to get rid of the mountain 'cause they kept fallin' off. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Do you have a phone? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="8" texto="I had it pulled.  People callin' all the time.  Who needs it -- Who'd you wanna call? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="4" texto="I wanna let my brother know where I am. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="D'you really wanna call? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Yes, I do. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="You sure? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Yes. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Why? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="I think he might be worried. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="I'll call your brother. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky flings open the window and bellows like a foghorn." duracion="8" infoExtraAccion="" parte="2" tiempoDescrip="2">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="6" texto="!!Yo, Paulie -- Ya sister's with me!  I'll call ya later. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky closes the window and faces the woman... She is not smiling.  She looks frightened." duracion="32" infoExtraAccion="" parte="3" tiempoDescrip="14">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="6" texto="What's the matter?  Ya don't like the room? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="It's fine. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="It's only temporary. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="It's not that -- " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="What's the problem?  You don't like me -- Don't like the turtles -- What is it? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" I don't think I belong here." duracion="50" infoExtraAccion="" parte="4" tiempoDescrip="16">
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="It's okay. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="No, I don't belong here. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="4" texto="It's all right -- You're my guest. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="4" texto="... I've never been in a man's apartment before. " vozPersonaje=""/>
                    <charDialog acotacion="(gesturing)" char="ROCKY" tdialog="4" texto="They're all the same. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="6" texto="I'm not sure I know you well enough -- I don't think I'm comfortable. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Yo, I'm not comfortable either. " vozPersonaje=""/>
                    <charDialog acotacion="(standing)" char="ADRIAN" tdialog="4" texto="I should leave. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="But I'm willin' to make the best of this uncomfortable situation. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Adrian moves to the door... Rocky intercepts her." duracion="12" infoExtraAccion="" parte="5" tiempoDescrip="4">
                    <charDialog acotacion="(continuing; softly)" char="ROCKY" tdialog="4" texto="Would ya take off your glasses? " vozPersonaje=""/>
                    <charDialog acotacion="(dumbstruck)" char="ADRIAN" tdialog="4" texto="What? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" The glasses... Please. Rocky removes her glasses and looks deeply into her eyes." duracion="20" infoExtraAccion="" parte="6" tiempoDescrip="10">
                    <charDialog acotacion="(timidly)" char="ADRIAN" tdialog="4" texto="... T-thank you. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Do me another favor? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="... What? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Could ya take off that hat. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" After a moment, Adrian removes the hat... She is becoming rather pretty." duracion="12" infoExtraAccion="" parte="7" tiempoDescrip="6">
                    <charDialog acotacion="(continuing)" char="ROCKY" tdialog="4" texto="I always knew you was pretty. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ADRIAN" tdialog="2" texto="Don't tease me. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" The woman melts into the corner and begins lightly sobbing... Rocky steps forward and fences her with his arms and body." duracion="12" infoExtraAccion="" parte="8" tiempoDescrip="6">
                    <charDialog acotacion="" char="ROCKY" tdialog="6" texto="I wanna kiss ya -- Ya don't have to kiss me back if ya don't feel like it. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky softly kisses the woman... Her arms hang limp.  He puts more passion into the kiss and she starts to respond. Her hand glides like smoke up his back. She embraces his neck.  The dam of passion erupts.  She gives herself freely for the first time in thirty years." duracion="10" parte="9" tiempoDescrip="10"/>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" GOLDMILL'S GYM .EXT." duracion="0" letra="." numeroCabecera="40" step="40" stepReal="40" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" GOLDMILL'S GYM " pin="0" principal="1" tipo="1" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <accion descripcion=" The following day, Rocky strolls down the street to Goldmill's Gym... Out front a group of young blacks stop talking and study Rocky as he passes.  Rocky's eyebrows knit in confusion." duracion="8" parte="0" tiempoDescrip="8"/>
            </pointGroup>
        </timeSlice>
        <timeSlice cabecera=" GOLDMILL'S GYM .INT." duracion="78" letra="." numeroCabecera="41" step="41" stepReal="41" time="">
            <pointGroup ambiente="-1" dummy="1" indiceAsignado="-1" name=" GOLDMILL'S GYM " pin="0" principal="1" tipo="0" tipoEscena="NORMAL" urlImagen="" y="300">
                <charPoint char="MIKE" label="1" tpar="100"/>
                <charPoint char="ROCKY" label="1" tpar="100"/>
                <charPoint char="MICKEY" label="1" tpar="100"/>
                <charPoint char="DIPPER" label="1" tpar="100"/>
                <charPoint char="JERGENS" label="1" tpar="100"/>
                <accion descripcion=" Rocky climbs the stairs and enters the gym... athletes stare in wonderment... The big black heavyweight contender, Dipper, throws down his towel in disgust and turns away. Mike quietly approaches Rocky." duracion="20" infoExtraAccion="" parte="0" tiempoDescrip="16">
                    <charDialog acotacion="" char="MIKE" tdialog="2" texto="Hey, Rock -- What happened? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="'Bout what? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Mickey Goldmill steps out of his office..." duracion="10" infoExtraAccion="" parte="1" tiempoDescrip="6">
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="Did ya get the message, kid? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Message -- What message? " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Mickey pulls out a card from his breast pocket... He hands it to Rocky." duracion="32" infoExtraAccion="" parte="2" tiempoDescrip="16">
                    <charDialog acotacion="" char="MICKEY" tdialog="8" texto="A Rep from Miles Jergens' Promotions was lookin' for ya -- They need sparrin' partners for Creed. " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="Ya puttin' me on? " vozPersonaje=""/>
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="Here's the card? " vozPersonaje=""/>
                    <charDialog acotacion="" char="ROCKY" tdialog="2" texto="When was they here? " vozPersonaje=""/>
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="'Bout an hour ago. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Probably lookin' for sparrin' partners." duracion="8" infoExtraAccion="" parte="3" tiempoDescrip="6">
                    <charDialog acotacion="" char="MICKEY" tdialog="2" texto="... I said that before. " vozPersonaje=""/>
                </accion>
                <accion descripcion=" Rocky turns from Mickey and jogs out of the gym.  Mickey fumbles with his rosary beads." duracion="8" infoExtraAccion="" parte="4" tiempoDescrip="4">
                    <charDialog acotacion="(continuing)" char="MICKEY" tdialog="4" texto="... Waste of life. " vozPersonaje=""/>
                </accion>
            </pointGroup>
